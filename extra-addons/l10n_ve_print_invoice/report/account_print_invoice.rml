<?xml version="1.0"?>
<document filename="Invoices.pdf">
  <template title="Invoices">
    <pageTemplate id="first">
      <frame id="first" x1="1.5cm" y1="1.0cm" width="18.59cm" height="21.5cm"/>
      <frame id="second" x1="1.5cm" y1="1.0cm" width="18.59cm" height="5.5cm"/>
    </pageTemplate>
  </template>
  <pageGraphics>
  	<setFont name="DejaVu Sans" size="8"/>
  	<rotate degrees="90"/>
