  	<drawString x="2.65cm" y="-0.4cm">generated by OpenERP</drawString>
  	<fill color="black"/>
  	<rotate degrees="-90"/>
  </pageGraphics>
  <stylesheet>
    <blockTableStyle id="Standard_Outline">
      <blockAlignment value="LEFT"/>
      <blockValign value="TOP"/>
    </blockTableStyle>
    <blockTableStyle id="Tabla1">
      <blockAlignment value="LEFT"/>
